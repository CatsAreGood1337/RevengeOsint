<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Email">

    <Button
        android:id="@+id/email1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="11dp"
        android:onClick="onEmail1"
        android:text="Intelligen"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:backgroundTint="#95A0AA"
        app:layout_constraintBottom_toTopOf="@+id/email2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email4" />

    <Button
        android:id="@+id/email2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="11dp"
        android:onClick="onEmail2"
        android:text="EmailRep"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:backgroundTint="#95A0AA"
        app:layout_constraintBottom_toTopOf="@+id/email3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email1" />

    <Button
        android:id="@+id/email3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="11dp"
        android:onClick="onEmail3"
        android:text="SpiderFoot"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:backgroundTint="#95A0AA"
        app:layout_constraintBottom_toTopOf="@+id/email5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email2" />

    <Button
        android:id="@+id/email4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="11dp"
        android:onClick="onEmail4"
        android:text="last4mail"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:backgroundTint="#95A0AA"
        app:layout_constraintBottom_toTopOf="@+id/email1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/email5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="471dp"
        android:onClick="onEmail5"
        android:text="EmailSherlock"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:backgroundTint="#95A0AA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email3" />

</androidx.constraintlayout.widget.ConstraintLayout>